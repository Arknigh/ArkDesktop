             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ArkDesktopHelperWPF"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
